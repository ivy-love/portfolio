때 add\r\n        console.log(currentScrollY, 'currentScrollY')\r\n        header.classList.add(\"is-show\");\r\n      } else {\r\n        console.log(this.lastScrollY, 'this.lastScrollY')\r\n        console.log('remove')\r\n        // 스크롤 올릴 때 remove\r\n        header.classList.remove(\"is-show\");\r\n      }\r\n\r\n      this.lastScrollY = currentScrollY;\r\n\r\n\r\n\r\n      // 기존\r\n      // let header = document.querySelector(\".portfolio-header\");\r\n      // let currentScrollY = window.scrollY;\r\n\r\n      // if (currentScrollY === 0 || currentScrollY < this.lastScrollY || currentScrollY < 84) {\r\n      //   header.classList.remove(\"is-show\");\r\n      // } else if(currentScrollY > this.lastScrollY) {\r\n      //   header.classList.add(\"is-show\");\r\n      // }\r\n\r\n      // this.lastScrollY = currentScrollY;\r\n\r\n\r\n\r\n\r\n      // let header = document.querySelector(\".portfolio-header\");\r\n      // let currentScrollY = window.scrollY;\r\n\r\n      // // 첫 번째 실행 시 lastScrollY 초기화\r\n      // if (this.lastScrollY === undefined) {\r\n      //   this.lastScrollY = 0;\r\n      // }\r\n\r\n      // if (currentScrollY === 0 || currentScrollY < this.lastScrollY || currentScrollY < 84) {\r\n      //   header.classList.remove(\"is-show\");\r\n      // } else if (currentScrollY > this.lastScrollY) {\r\n      //   header.classList.add(\"is-show\");\r\n      // }\r\n\r\n      // this.lastScrollY = currentScrollY;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n"],"mappings":";;EAESA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAS;;EAEbA,KAAK,EAAC;AAAS;;EACdA,KAAK,EAAC;AAAU;mBAN9B;;uBACEC,mBAAA,CAaM;IAbDD,KAAK,EADZE,eAAA,EACa,kBAAkB;MAAA,YAAuBC,IAAA,CAAAC;IAAQ;MAC1DC,mBAAA,CAWM,OAXNC,UAWM,GAVJD,mBAAA,CASM,OATNE,UASM,G,0BARJF,mBAAA,CAAmD;IAA/CL,KAAK,EAAC;EAAM,IAACK,mBAAA,CAA6B;IAA1BG,IAAI,EAAC;EAAG,IAJpCC,gBAAA,CAIqC,MAAI,GAAAJ,mBAAA,CAAI,OAJ7CI,gBAAA,CAI6C,OAAK,E,wBAC1CJ,mBAAA,CAMM,OANNK,UAMM,GALJL,mBAAA,CAIK,MAJLM,UAIK,I,kBAHHV,mBAAA,CAEKW,SAAA,QATjBC,WAAA,CAOgDC,KAAA,C