t>\r\nexport default {\r\n  name: \"LayoutHeader\",\r\n  data() {\r\n    return {\r\n      isOpen: false,\r\n      isActive: false,\r\n      gnbList: ['About', 'Works', 'Contact'],\r\n    }\r\n  },\r\n  mounted () {\r\n    this.worksAni();\r\n    this.txtScroll();\r\n    this.goMenu();\r\n    this.handleScroll();\r\n  },\r\n  methods: {\r\n    // 모바일 메뉴 열렸을 때 body overflow 추가 및 제거\r\n    toggleMoBtn() {\r\n      this.isOpen = !this.isOpen;\r\n      if (this.isOpen) {\r\n        document.body.classList.add('is-active');\r\n      }\r\n      else {\r\n        document.body.classList.remove('is-active')\r\n      }\r\n    },\r\n\r\n    // works-section 애니메이션\r\n    worksAni() {\r\n      let worksSection = document.querySelector('.works-section');\r\n      window.addEventListener('scroll', function() {\r\n        let sectionRect = worksSection.getBoundingClientRect();\r\n        if (sectionRect.top * 0.5 <= window.innerHeight && sectionRect.bottom >= 0) {\r\n          worksSection.classList.add('is-active');\r\n        } else {\r\n          worksSection.classList.remove('is-active');\r\n        }\r\n      });\r\n    },\r\n\r\n    // gnb메뉴 text-section 지났을 때 클래스 추가\r\n    txtScroll() {\r\n      let header = document.querySelector('.portfolio-header');\r\n      let textSection = document.querySelector('.text-section');\r\n      const vm = this;\r\n\r\n      window.addEventListener('scroll', function() {\r\n        if (vm.isOpen) {\r\n          header.classList.remove('is-color');\r\n          return;\r\n        }\r\n\r\n        let sectionRect = textSection.getBoundingClientRect();\r\n\r\n        if (sectionRect.top <= 0) {\r\n          header.classList.add('is-color');\r\n        } else {\r\n          header.classList.remove('is-color');\r\n        }\r\n      });\r\n    },\r\n\r\n    // gnb메뉴 이동 버튼\r\n    goMenu(section) {\r\n      const targetSection = document.querySelector(`#${section}`);\r\n      if (targetSection) {\r\n        targetSection.scrollIntoView({ behavior: \"smooth\" });\r\n      }\r\n    },\r\n\r\n    // 스크롤 시 헤더 숨기기\r\n    handleScroll () {\r\n      let lastScrollY = 0;\r\n      const vm = this;\r\n\r\n      window.addEventListener(\"scroll\", function () {\r\n        if (vm.isOpen) return;\r\n\r\n        let header = document.querySelector(\".portfolio-header\");\r\n        let currentScrollY = window.scrollY;\r\n\r\n        if (currentScrollY === 0 || currentScrollY < lastScrollY || currentScrollY < 84) {\r\n            header.classList.remove(\"is-scroll\");\r\n        } else if (currentScrollY > lastScrollY) {\r\n            header.classList.add(\"is-scroll\");\r\n        }\r\n\r\n        lastScrollY = currentScrollY;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n"],"mappings":";;EAESA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAS;;EAGbA,KAAK,EAAC;AAAkB;;EACvBA,KAAK,EAAC;AAAU;mBAP9B;;;uBACEC,mBAAA,CAmBM;IAnBDD,KAAK,EADZE,eAAA,EACa,kBAAkB;MAAA,YAAuBC,KAAA,CAAAC;IAAQ;MAC1DC,mBAAA,CAiBM,OAjBNC,UAiBM,GAhBJD,mBAAA,CAeM,OAfNE,UAeM,GAdJC,mBAAA,6FAA4F,EAC5FC,YAAA,CAAgEC,sBAAA;IAAlDC,EAAE,EAAE;MAAAC,IAAA;IAAA;EAAiB;IAL3CC,OAAA,EAAAC,QAAA,CAK6C,MAAIC,MAAA,QAAAA,MAAA,OALjDC,gBAAA,CAK6C,MAAI,GAAAX,mBAAA,CAAI,qCALrDW,gBAAA,CAKqD,OAAK,E;IAL1DC,CAAA;MAMQZ,mBAAA,CAMM,OANNa,UAMM,GALJb,mBAAA,CAIK,MAJLc,UAIK,I,kBAHHlB,mBAAA,CAEKmB,SAAA,QAVjBC,WAAA,CAQgDlB,KAAA,CAAAmB,OAAO,EAAfC,IAAI;yBAAhCtB,mBAAA,CAEK;MAFDD,KAAK,EAAC,UAAU;MAA0BwB,GAAG,EAAED;QACjDlB,mBAAA,CAAwD;MAArDL,KAAK,EAAC,UAAU;MAAEyB,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAC,MAAM,CAACL,IAAI;wBAAMA,IAAI,wBAT/DM,UAAA,E;sCAaQxB,mBAAA,CAIS;IAJDyB,IAAI,EAAC,QAAQ;IAAC9B,KAAK,EAbnCE,eAAA,EAaoC,qBAAqB;MAAA,aAAwBC,KAAA,CAAA4B;IAAM;IAAKN,OAAK,EAAAV,MAAA,QAAAA,MAAA,UAAAiB,IAAA,KAAEL,QAAA,CAAAM,WAAA,IAAAN,QAAA,CAAAM,WAAA,IAAAD,IAAA,CAAW;gCACpG3B,mBAAA,CAEM;IAFDL,KAAK,EAAC;EAAa,IACtBK,mBAAA,CAA6B;IAAvBL,KAAK,EAAC;EAAS,G","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}