{"ast":null,"code":"import InfoItem from './components/InfoItem.vue';\nimport { gsap } from 'gsap';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\nimport 'gsap/ScrollToPlugin';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: \"Index\",\n  components: {\n    InfoItem\n  },\n  data() {\n    return {\n      workItems: [{\n        title: '포켓컴퍼니',\n        detail: '웹 퍼블리셔 / 사원',\n        period: '2023.09 - 2024.10'\n      }, {\n        title: '(주)씨케이앤비',\n        detail: '웹 디자이너 / 대리, 파트장',\n        period: '2019.03 - 2021.01'\n      }, {\n        title: '(주)대해이앤지',\n        detail: '경리 / 대리',\n        period: '2015.11 - 2018.09'\n      }, {\n        title: '특허법인 누리',\n        detail: '특허 도면사 / 대리',\n        period: '2012.01 - 2014.02'\n      }, {\n        title: 'J&K 국제특허 사무소',\n        detail: '특허 도면사 / 대리',\n        period: '2009.07 - 2011.12'\n      }],\n      eduItems: [{\n        title: '리액트&Node.js',\n        period: '2023.04 - 2023.06'\n      }, {\n        title: 'JQuery&Javascript',\n        period: '2022.04 - 2022.06'\n      }, {\n        title: 'JQuery&Javascript',\n        period: '2020.10 - 2020.12'\n      }, {\n        title: 'HTML&CSS',\n        period: '2020.05 - 2020.08'\n      }, {\n        title: '웹표준&접근성',\n        period: '2020.02 - 2020.03'\n      }],\n      licenseItems: [{\n        title: '컴퓨터 그래픽스 운용기능사',\n        period: '2018.09'\n      }]\n    };\n  },\n  mounted: function () {\n    this.scrollAnimation();\n    this.textScrollAnimation();\n  },\n  methods: {\n    scrollAnimation() {\n      ScrollTrigger.config({\n        autoRefreshEvents: \"visibilitychange,DOMContentLoaded,load\"\n      });\n      const containers = gsap.utils.toArray(\".intro-section\");\n      containers.forEach((cont, i) => {\n        const slides = Array.from(cont.querySelectorAll(\".slide\"));\n        const innerHeight = window.innerHeight;\n\n        // 전체 애니메이션 범위 계산\n        const marginValue = innerHeight * (slides.length - 1);\n        const totalScrollHeight = marginValue + innerHeight;\n        ScrollTrigger.create({\n          trigger: cont,\n          start: \"top top\",\n          end: \"+=\" + totalScrollHeight,\n          pin: true,\n          pinSpacing: true,\n          // 섹션 간의 간격 유지\n          markers: true\n        });\n\n        // 애니메이션 타임라인 생성\n        gsap.timeline({\n          scrollTrigger: {\n            trigger: cont,\n            start: \"top top\",\n            end: \"+=\" + totalScrollHeight,\n            scrub: true,\n            markers: true\n          }\n        }).to(slides, {\n          xPercent: -(100 * (slides.length - 1)),\n          // 슬라이드가 연속적으로 이동\n          duration: slides.length,\n          ease: \"none\"\n        });\n      });\n    },\n    textScrollAnimation() {\n      gsap.to(\".txt01\", {\n        scrollTrigger: {\n          trigger: \".intro-txt-grp\",\n          start: \"top center\",\n          // 텍스트가 화면 중간에 들어오면 시작\n          end: \"bottom center\",\n          scrub: true,\n          markers: true // 디버깅을 위해 마커 활성화\n        },\n        x: 130,\n        ease: \"none\"\n      });\n      gsap.to(\".txt02\", {\n        scrollTrigger: {\n          trigger: \".intro-txt-grp\",\n          start: \"top center\",\n          // 텍스트가 화면 중간에 들어오면 시작\n          end: \"bottom center\",\n          scrub: true,\n          markers: true\n        },\n        x: 300,\n        // 이동 거리\n        ease: \"none\"\n      });\n      gsap.to(\".txt03\", {\n        scrollTrigger: {\n          trigger: \".intro-txt-grp\",\n          start: \"top center\",\n          // 텍스트가 화면 중간에 들어오면 시작\n          end: \"bottom center\",\n          scrub: true,\n          markers: true\n        },\n        x: 200,\n        // 이동 거리\n        ease: \"none\"\n      });\n    }\n  }\n};","map":{"version":3,"names":["InfoItem","gsap","ScrollTrigger","registerPlugin","name","components","data","workItems","title","detail","period","eduItems","licenseItems","mounted","scrollAnimation","textScrollAnimation