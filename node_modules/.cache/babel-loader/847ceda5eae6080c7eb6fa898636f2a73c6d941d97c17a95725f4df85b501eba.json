{"ast":null,"code":"/* import InfoItem from './components/InfoItem.vue' */\nimport { gsap } from 'gsap';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\nimport 'gsap/ScrollToPlugin';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: \"Index\",\n  components: {\n    // InfoItem,\n  },\n  data() {\n    return {\n      workItems: [{\n        title: '포켓컴퍼니',\n        detail: '웹 퍼블리셔',\n        period: '2023.09 - 2024.10'\n      }, {\n        title: '(주)씨케이앤비',\n        detail: '웹 디자이너 / 대리, 파트장',\n        period: '2019.03 - 2021.01'\n      }, {\n        title: '(주)대해이앤지',\n        detail: '경리 / 대리',\n        period: '2015.11 - 2018.09'\n      }, {\n        title: '특허법인 누리',\n        detail: '특허 도면사 / 대리',\n        period: '2012.01 - 2014.02'\n      }, {\n        title: 'J&K 국제특허 사무소',\n        detail: '특허 도면사 / 대리',\n        period: '2009.07 - 2011.12'\n      }],\n      eduItems: [{\n        title: '리액트&Node.js',\n        period: '2023.04 - 2023.06'\n      }, {\n        title: 'JQuery&Javascript',\n        period: '2022.04 - 2022.06'\n      }, {\n        title: 'JQuery&Javascript',\n        period: '2020.10 - 2020.12'\n      }, {\n        title: 'HTML&CSS',\n        period: '2020.05 - 2020.08'\n      }, {\n        title: '웹표준&접근성',\n        period: '2020.02 - 2020.03'\n      }],\n      licenseItems: [{\n        title: '컴퓨터 그래픽스 운용기능사',\n        period: '2018.09'\n      }]\n    };\n  },\n  mounted: function () {\n    this.scrollAnimation();\n  },\n  methods: {\n    scrollAnimation() {\n      const containers = gsap.utils.toArray(\".intro-section\");\n      containers.forEach((cont, i) => {\n        const slides = Array.from(cont.querySelectorAll(\".slide\"));\n        if (slides.length === 0) {\n          console.warn(`No slides found in container #${i}:`, cont);\n          return;\n        }\n        const innerHeight = window.innerHeight;\n        const marginValue = innerHeight * slides.length;\n        const finalText = slides[slides.length - 1]?.querySelector(\"h1\");\n        gsap.set(cont, {\n          marginBottom: marginValue\n        });\n        ScrollTrigger.create({\n          trigger: cont,\n          start: \"top top\",\n          end: \"+=\" + (marginValue + innerHeight),\n          pin: true,\n          pinSpacing: false,\n          markers: true\n        });\n        gsap.timeline({\n          scrollTrigger: {\n            trigger: cont,\n            start: \"top top\",\n            end: \"+=\" + (marginValue + innerHeight),\n            scrub: true,\n            markers: true\n          }\n        }).to(slides, {\n          xPercent: -(100 * (slides.length - 1)),\n          duration: slides.length,\n          ease: \"none\"\n        }).to(finalText || {},\n        // finalText가 없으면 빈 객체를 전달\n        {\n          duration: 1,\n          rotation: 360,\n          opacity: 0,\n          ease: \"none\"\n        }, \"-=0.5\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["gsap","ScrollTrigger","registerPlugin","name","components","data","workItems","title","detail","period","eduItems","licenseItems","mounted","scrollAnimation","methods","containers","utils","toArray","forEach","cont","i","slides","Array","from","querySelectorAll","length","console","warn","innerHeight","window","marginValue","finalText","querySelector","set","marginBottom","create","trigger","start","end","pin","pinSpacing","markers","timeline","scrollTrigger","scrub","to","xPercent","duration","ease","rotation","opacity"],"sources":["/Users/jiheek/Desktop/doc/개인/20241129_포트폴리오/portfiolio/src/views/index.vue"],"sourcesContent":["<template>\n  <LayoutHeader />\n  <div class=\"page\">\n    <!-- main-section START -->\n    <section class=\"main-section\">\n      <h1 class=\"voice-only\">메인 섹션</h1>\n      <div class=\"main-video-grp\">\n        <video\n          autoplay\n          muted\n          loop\n          playsinline\n          disablePictureInPicture\n          controlsList=\"nodownload\"\n          class=\"video\"\n        >\n          <source\n            src=\"@/assets/video/vdo_typing.mp4\"\n            type=\"video/mp4\"\n          >\n        </video>\n      </div>\n      <div class=\"main-wrap\">\n        <div class=\"txt-wrap\">\n          <span class=\"main-txt first green\">Your vision</span><i class=\"icon icon-star\"></i>\n          <span class=\"main-txt wh\">my <span class=\"line\"></span>passion</span>\n          <span class=\"main-txt third green\">a perfect web reality.</span>\n        </div>\n        <button type=\"button\" class=\"btn btn-arrow-wh scroll-vertical-img\">\n          <i class=\"icon icon-arrow-wh\"></i>\n        </button>\n      </div>\n    </section>\n    <!-- main-section END -->\n\n     <div class=\"intro-section\">\n      <!-- intro-section-01 START -->\n      <section class=\"intro-section-01 slide\">\n        <h1 class=\"voice-only\">인트로 섹션1</h1>\n        <div class=\"profile-grp-01\">\n          <img src=\"@/assets/images/img_profile_01.svg\" alt=\"프로필 이미지1\" class=\"img\">\n        </div>\n        <div class=\"intro-txt-grp\">\n          <p class=\"txt green\">I AM</p>\n          <p class=\"txt\">Kim</p>\n          <p class=\"txt\">Ji hee</p>\n        </div>\n      </section>\n      <!-- intro-section-01 END -->\n\n      <!-- intro-section-02 START -->\n      <section class=\"intro-section-02 slide\">\n        <h1 class=\"voice-only\">인트로 섹션2</h1>\n        <div class=\"content-wrap\">\n          <div class=\"profile-grp-02\">\n            <img src=\"@/assets/images/img_profile_02.svg\" alt=\"프로필 이미지\" class=\"img\">\n          </div>\n          <div class=\"txt-grp\">\n            <p class=\"txt\">\n              웹디자이너로 근무하며 코딩으로 브라우저에 결과물을 나타내는 것에 흥미를 느껴 웹 퍼블리셔에 도전하고 있습니다. 탄탄한 기본기가 중요하다고 생각하여\n              <br>\n              <br>\n              웹표준을 기반으로 한 컴포넌트 단위의 최적화된 마크업을 구현하려고 집중합니다. 웹디자이너로 근무하며 코딩으로 브라우저에 결과물을 나타내는 것에 흥미를 느껴 웹 퍼블리셔에 도전하고 있습니다.\n              <br>\n              <br>\n              탄탄한 기본기가 중요하다고 생각하여 웹표준을 기반으로 한 컴포넌트 단위의 최적화된 마크업을 구현하려고 집중합니다.\n            </p>\n            <button class=\"btn btn-round-wh\" type=\"button\">\n              Go to Resume\n              <i class=\"icon icon-arrow-right-bk\"></i>\n            </button>\n          </div>\n        </div>\n      </section>\n      <!-- intro-section-02 END -->\n    </div>\n\n    <!-- about-section START -->\n     <section class=\"about-section\">\n      <h1 class=\"voice-only\">어바웃 섹션</h1>\n      <div class=\"l-center\">\n        <div class=\"about-wrap\">\n          <div class=\"profile-grp\">\n            <div class=\"img-grp\">\n              <!-- <img src=\"../assets/images/profile.svg\" alt=\"프로필 이미지\" class=\"img\"> -->\n            </div>\n            <p class=\"profile-txt\">\n              웹디자이너로 근무하며 코딩으로 브라우저에 결과물을 나타내는 것에 흥미를 느껴 웹 퍼블리셔에 도전하고 있습니다. 탄탄한 기본기가 중요하다고 생각하여 웹표준을 기반으로 한 컴포넌트 단위의 최적화된 마크업을 구현하려고 집중합니다.\n            </p>\n            <a class=\"more-view\" href=\"\">이력서 더보기<i class=\"icon icon-arrow-right scroll-horizontal-img\"></i></a>\n          </div>\n          <div class=\"info-wrap\">\n            <h3 class=\"info-title\">Work Experience</h3>\n            workItems\n            <InfoItem\n              v-for=\"(item, idx) in workItems\"\n              :key=\"idx\"\n              :title=\"item.title\"\n              :detail=\"item.detail\"\n              :period=\"item.period\"\n            />\n            Skills\n            <div class=\"skills-grp\">\n              <h3 class=\"info-title\">Skills</h3>\n              <p class=\"skill-items\">\n                Html, CSS, Javascript, Jquery, Vue, Scss, Tailwind\n                <br>\n                Git, Figma, Adobe XD, Zeplin, Slack, Jira, Photoshop, illustration\n              </p>\n            </div>\n            <div class=\"half-grp\">\n              <div class=\"half\">\n                <h3 class=\"info-title\">Education</h3>\n                <InfoItem\n                  v-for=\"(item, idx) in eduItems\"\n                  :key=\"idx\"\n                  :title=\"item.title\"\n                  :period=\"item.period\"\n                />\n              </div>\n              <div class=\"half\">\n                <h3 class=\"info-title\">license</h3>\n                <InfoItem\n                  v-for=\"(item, idx) in licenseItems\"\n                  :key=\"idx\"\n                  :title=\"item.title\"\n                  :period=\"item.period\"\n                />\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n     </section>\n     <!-- about-section END -->\n\n     <!-- passion-section START -->\n     <div class=\"passion-section\">\n      <div class=\"l-center\">\n        <h5 class=\"passion-txt\">\n          도전을 두려워하지 않는 <br>\n          열정❤️‍🔥 가득한 웹 퍼블리셔,<br>\n          김지희입니다.\n        </h5>\n      </div>\n     </div>\n     <!-- passion-section END -->\n  </div>\n</template>\n<script>\n/* import InfoItem from './components/InfoItem.vue' */\nimport { gsap } from 'gsap'\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\nimport 'gsap/ScrollToPlugin';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: \"Index\",\n  components: {\n    // InfoItem,\n  },\n  data() {\n    return {\n      workItems: [\n        {\n          title: '포켓컴퍼니',\n          detail: '웹 퍼블리셔',\n          period: '2023.09 - 2024.10'\n        },\n        {\n          title: '(주)씨케이앤비',\n          detail: '웹 디자이너 / 대리, 파트장',\n          period: '2019.03 - 2021.01'\n        },\n        {\n          title: '(주)대해이앤지',\n          detail: '경리 / 대리',\n          period: '2015.11 - 2018.09'\n        },\n        {\n          title: '특허법인 누리',\n          detail: '특허 도면사 / 대리',\n          period: '2012.01 - 2014.02'\n        },\n        {\n          title: 'J&K 국제특허 사무소',\n          detail: '특허 도면사 / 대리',\n          period: '2009.07 - 2011.12'\n        }\n      ],\n      eduItems: [\n        {\n          title: '리액트&Node.js',\n          period: '2023.04 - 2023.06'\n        },\n        {\n          title: 'JQuery&Javascript',\n          period: '2022.04 - 2022.06'\n        },\n        {\n          title: 'JQuery&Javascript',\n          period: '2020.10 - 2020.12'\n        },\n        {\n          title: 'HTML&CSS',\n          period: '2020.05 - 2020.08'\n        },\n        {\n          title: '웹표준&접근성',\n          period: '2020.02 - 2020.03'\n        },\n      ],\n      licenseItems: [\n        {\n          title: '컴퓨터 그래픽스 운용기능사',\n          period: '2018.09'\n        },\n      ],\n    }\n  },\n  mounted: function() {\n    this.scrollAnimation();\n  },\n  methods: {\n    scrollAnimation() {\n    const containers = gsap.utils.toArray(\".intro-section\");\n\n    containers.forEach((cont, i) => {\n      const slides = Array.from(cont.querySelectorAll(\".slide\"));\n      if (slides.length === 0) {\n        console.warn(`No slides found in container #${i}:`, cont);\n        return;\n      }\n\n      const innerHeight = window.innerHeight;\n      const marginValue = innerHeight * slides.length;\n      const finalText = slides[slides.length - 1]?.querySelector(\"h1\");\n\n      gsap.set(cont, {\n        marginBottom: marginValue,\n      });\n\n      ScrollTrigger.create({\n        trigger: cont,\n        start: \"top top\",\n        end: \"+=\" + (marginValue + innerHeight),\n        pin: true,\n        pinSpacing: false,\n        markers: true,\n      });\n\n      gsap\n        .timeline({\n          scrollTrigger: {\n            trigger: cont,\n            start: \"top top\",\n            end: \"+=\" + (marginValue + innerHeight),\n            scrub: true,\n            markers: true,\n          },\n        })\n        .to(slides, {\n          xPercent: -(100 * (slides.length - 1)),\n          duration: slides.length,\n          ease: \"none\",\n        })\n        .to(\n          finalText || {}, // finalText가 없으면 빈 객체를 전달\n          {\n            duration: 1,\n            rotation: 360,\n            opacity: 0,\n            ease: \"none\",\n          },\n          \"-=0.5\"\n        );\n    });\n  }\n\n  }\n};\n</script>"],"mappings":"AAsJA;AACA,SAASA,IAAG,QAAS,MAAK;AAC1B,SAASC,aAAY,QAAS,oBAAoB;AAClD,OAAO,qBAAqB;AAC5BD,IAAI,CAACE,cAAc,CAACD,aAAa,CAAC;AAClC,eAAe;EACbE,IAAI,EAAE,OAAO;EACbC,UAAU,EAAE;IACV;EAAA,CACD;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT;QACEC,KAAK,EAAE,OAAO;QACdC,MAAM,EAAE,QAAQ;QAChBC,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,UAAU;QACjBC,MAAM,EAAE,kBAAkB;QAC1BC,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,UAAU;QACjBC,MAAM,EAAE,SAAS;QACjBC,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,SAAS;QAChBC,MAAM,EAAE,aAAa;QACrBC,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,cAAc;QACrBC,MAAM,EAAE,aAAa;QACrBC,MAAM,EAAE;MACV,EACD;MACDC,QAAQ,EAAE,CACR;QACEH,KAAK,EAAE,aAAa;QACpBE,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,mBAAmB;QAC1BE,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,mBAAmB;QAC1BE,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,UAAU;QACjBE,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,SAAS;QAChBE,MAAM,EAAE;MACV,CAAC,CACF;MACDE,YAAY,EAAE,CACZ;QACEJ,KAAK,EAAE,gBAAgB;QACvBE,MAAM,EAAE;MACV,CAAC;IAEL;EACF,CAAC;EACDG,OAAO,EAAE,SAAAA,CAAA,EAAW;IAClB,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACPD,eAAeA,CAAA,EAAG;MAClB,MAAME,UAAS,GAAIf,IAAI,CAACgB,KAAK,CAACC,OAAO,CAAC,gBAAgB,CAAC;MAEvDF,UAAU,CAACG,OAAO,CAAC,CAACC,IAAI,EAAEC,CAAC,KAAK;QAC9B,MAAMC,MAAK,GAAIC,KAAK,CAACC,IAAI,CAACJ,IAAI,CAACK,gBAAgB,CAAC,QAAQ,CAAC,CAAC;QAC1D,IAAIH,MAAM,CAACI,MAAK,KAAM,CAAC,EAAE;UACvBC,OAAO,CAACC,IAAI,CAAC,iCAAiCP,CAAC,GAAG,EAAED,IAAI,CAAC;UACzD;QACF;QAEA,MAAMS,WAAU,GAAIC,MAAM,CAACD,WAAW;QACtC,MAAME,WAAU,GAAIF,WAAU,GAAIP,MAAM,CAACI,MAAM;QAC/C,MAAMM,SAAQ,GAAIV,MAAM,CAACA,MAAM,CAACI,MAAK,GAAI,CAAC,CAAC,EAAEO,aAAa,CAAC,IAAI,CAAC;QAEhEhC,IAAI,CAACiC,GAAG,CAACd,IAAI,EAAE;UACbe,YAAY,EAAEJ;QAChB,CAAC,CAAC;QAEF7B,aAAa,CAACkC,MAAM,CAAC;UACnBC,OAAO,EAAEjB,IAAI;UACbkB,KAAK,EAAE,SAAS;UAChBC,GAAG,EAAE,IAAG,IAAKR,WAAU,GAAIF,WAAW,CAAC;UACvCW,GAAG,EAAE,IAAI;UACTC,UAAU,EAAE,KAAK;UACjBC,OAAO,EAAE;QACX,CAAC,CAAC;QAEFzC,IAAG,CACA0C,QAAQ,CAA