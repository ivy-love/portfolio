{"ast":null,"code":"import InfoItem from './components/InfoItem.vue';\nimport { gsap } from 'gsap';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\nimport 'gsap/ScrollToPlugin';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: \"Index\",\n  components: {\n    InfoItem\n  },\n  data() {\n    return {\n      workItems: [{\n        title: '포켓컴퍼니',\n        detail: '웹 퍼블리셔 / 사원',\n        period: '2023.09 - 2024.10'\n      }, {\n        title: '(주)씨케이앤비',\n        detail: '웹 디자이너 / 대리, 파트장',\n        period: '2019.03 - 2021.01'\n      }\n      // {\n      //   title: '(주)대해이앤지',\n      //   detail: '경리 / 대리',\n      //   period: '2015.11 - 2018.09'\n      // },\n      // {\n      //   title: '특허법인 누리',\n      //   detail: '특허 도면사 / 대리',\n      //   period: '2012.01 - 2014.02'\n      // },\n      // {\n      //   title: 'J&K 국제특허 사무소',\n      //   detail: '특허 도면사 / 대리',\n      //   period: '2009.07 - 2011.12'\n      // }\n      ],\n      eduItems: [{\n        title: '리액트&Node.js',\n        period: '2023.04 - 2023.06'\n      }, {\n        title: 'JQuery&Javascript',\n        period: '2022.04 - 2022.06'\n      }, {\n        title: 'JQuery&Javascript',\n        period: '2020.10 - 2020.12'\n      }, {\n        title: 'HTML&CSS',\n        period: '2020.05 - 2020.08'\n      }, {\n        title: '웹표준&접근성',\n        period: '2020.02 - 2020.03'\n      }],\n      licenseItems: [{\n        title: '컴퓨터 그래픽스 운용기능사',\n        period: '2018.09'\n      }]\n    };\n  },\n  mounted: function () {\n    this.scrollAnimation();\n    this.textScrollAnimation();\n    this.viewColorChange();\n  },\n  methods: {\n    scrollAnimation() {\n      ScrollTrigger.config({\n        autoRefreshEvents: \"visibilitychange,DOMContentLoaded,load\"\n      });\n      const containers = gsap.utils.toArray(\".intro-section\");\n      containers.forEach((cont, i) => {\n        const slides = Array.from(cont.querySelectorAll(\".slide\"));\n        const innerHeight = window.innerHeight;\n\n        // 전체 애니메이션 범위 계산\n        const marginValue = innerHeight * (slides.length - 1);\n        const totalScrollHeight = marginValue + innerHeight;\n        ScrollTrigger.create({\n          trigger: cont,\n          start: \"top top\",\n          end: \"+=\" + totalScrollHeight,\n          pin: true,\n          pinSpacing: true,\n          // 섹션 간의 간격 유지\n          markers: true\n        });\n\n        // 애니메이션 타임라인 생성\n        gsap.timeline({\n          scrollTrigger: {\n            trigger: cont,\n            start: \"top top\",\n            end: \"+=\" + totalScrollHeight,\n            scrub: true,\n            markers: true\n          }\n        }).to(slides, {\n          xPercent: -(100 * (slides.length - 1)),\n          // 슬라이드가 연속적으로 이동\n          duration: slides.length,\n          ease: \"none\"\n        });\n      });\n    },\n    textScrollAnimation() {\n      gsap.to(\".txt01\", {\n        scrollTrigger: {\n          trigger: \".intro-txt-grp\",\n          start: \"top center\",\n          // 텍스트가 화면 중간에 들어오면 시작\n          end: \"bottom center\",\n          scrub: true,\n          markers: true // 디버깅을 위해 마커 활성화\n          // scroller: \".page\",\n        },\n        x: 130,\n        ease: \"none\"\n      });\n      gsap.to(\".txt02\", {\n        scrollTrigger: {\n          trigger: \".intro-txt-grp\",\n          start: \"top center\",\n          // 텍스트가 화면 중간에 들어오면 시작\n          end: \"bottom center\",\n          scrub: true,\n          markers: true\n        },\n        x: 300,\n        // 이동 거리\n        ease: \"none\"\n      });\n      gsap.to(\".txt03\", {\n        scrollTrigger: {\n          trigger: \".intro-txt-grp\",\n          start: \"top center\",\n          // 텍스트가 화면 중간에 들어오면 시작\n          end: \"bottom center\",\n          scrub: true,\n          markers: true\n        },\n        x: 200,\n        // 이동 거리\n        ease: \"none\"\n      });\n    },\n    // 화면 색깔 전환 애니메이션\n    viewColorChange() {\n      gsap.to(\".change\", {\n        scrollTrigger: {\n          trigger: \".change\",\n          start: \"center center\",\n          end: \"bottom bottom\",\n          scrub: true,\n          markers: true\n        },\n        duration: 0.01,\n        backgroundColor: \"#fff\",\n        ease: \"none\"\n      });\n    }\n  }\n};","map":{"version":3,"names":["InfoItem","gsap","ScrollTrigger","registerPlugin","name","components","data","workItems","title","detail","period","eduItems","licenseItems","mounted","scrollAnimation","textScrollAnimation","viewColorChange","methods","config","autoRefreshEvents","containers","utils","toArray","forEach","cont","i","slides","Array","from","querySelectorAll","innerHeight","window","marginValue","length","totalScrollHeight","create","trigger","start","end","pin","pinSpacing","markers","timeline","scrollTrigger","scrub","to","xPercent","duration","ease","x","backgroundColor"],"sources":["/Users/jiheek/Desktop/doc/개인/20241129_포트폴리오/portfiolio/src/views/index.vue"],"sourcesContent":["<template>\n  <LayoutHeader />\n  <div class=\"page\">\n    <!-- main-section START -->\n    <section class=\"main-section\">\n      <h1 class=\"voice-only\">메인 섹션</h1>\n      <div class=\"main-video-grp\">\n        <video\n          autoplay\n          muted\n          loop\n          playsinline\n          disablePictureInPicture\n          controlsList=\"nodownload\"\n          class=\"video\"\n        >\n          <source\n            src=\"@/assets/video/vdo_typing.mp4\"\n            type=\"video/mp4\"\n          >\n        </video>\n      </div>\n      <div class=\"main-wrap\">\n        <div class=\"txt-wrap\">\n          <span class=\"main-txt first green\">Your vision</span><i class=\"icon icon-star\"></i>\n          <span class=\"main-txt wh\">my <span class=\"line\"></span>passion</span>\n          <span class=\"main-txt third green\">a perfect web reality.</span>\n        </div>\n      </div>\n      <button type=\"button\" class=\"btn btn-arrow-wh scroll-vertical-img\">\n        <i class=\"icon icon-arrow-wh\"></i>\n      </button>\n    </section>\n    <!-- main-section END -->\n\n     <div class=\"intro-section\">\n      <!-- intro-section-01 START -->\n      <section class=\"intro-section-01 slide\">\n        <h1 class=\"voice-only\">인트로 섹션1</h1>\n        <div class=\"profile-grp-01\">\n          <img src=\"@/assets/images/img_profile_01.svg\" alt=\"프로필 이미지1\" class=\"img\">\n        </div>\n        <div class=\"intro-txt-grp\" data-aos=\"fade-up\" data-aos-duration=\"800\">\n          <p class=\"txt green txt01\">I AM</p>\n          <p class=\"txt txt02\">Kim</p>\n          <p class=\"txt txt03\">Ji hee</p>\n        </div>\n      </section>\n      <!-- intro-section-01 END -->\n\n      <!-- intro-section-02 START -->\n      <section class=\"intro-section-02 slide\">\n        <h1 class=\"voice-only\">인트로 섹션2</h1>\n        <div class=\"content-wrap\">\n          <div class=\"profile-grp-02\">\n            <img src=\"@/assets/images/img_profile_02.svg\" alt=\"프로필 이미지\" class=\"img\">\n          </div>\n          <div class=\"txt-grp\">\n            <p class=\"txt\">\n              웹 디자이너로 IT 업계에 첫 발을 내딛고, 코딩을 통해 브라우저에 결과물을 구현하는 과정에서 흥미를 느껴 웹 퍼블리셔의 길을 걷게 되었습니다.\n              <br>\n              <br>\n              저는 단순히 디자인을 화면에 옮기는 것에 그치지 않고, 사용자 관점에서 고민하며 더 나은 방향을 제시하는 협업을 추구합니다.\n              <br>\n              <br>\n              특히, 웹 표준을 준수하며 컴포넌트 단위로 최적화된 마크업을 구현하는 데 집중하고 있으며, 이를 통해 더 나은 결과물을 만들어내기 위해 꾸준히 성장하고자 노력합니다.\n            </p>\n            <button class=\"btn btn-round-wh\" type=\"button\">\n              Go to Resume\n              <i class=\"icon icon-arrow-right-bk scroll-horizontal-img\"></i>\n            </button>\n          </div>\n        </div>\n      </section>\n      <!-- intro-section-02 END -->\n    </div>\n\n    <div class=\"change\">\n      <!-- about-section START -->\n      <section class=\"about-section\">\n        <h1 class=\"voice-only\">어바웃 섹션</h1>\n        <div class=\"l-center\">\n          <div class=\"info-wrap\">\n            <h3 class=\"info-title\">Work Experience</h3>\n            <InfoItem\n              v-for=\"(item, idx) in workItems\"\n              :key=\"idx\"\n              :title=\"item.title\"\n              :detail=\"item.detail\"\n              :period=\"item.period\"\n            />\n          </div>\n          <div class=\"info-wrap\">\n            <h3 class=\"info-title\">Education</h3>\n            <InfoItem\n              v-for=\"(item, idx) in eduItems\"\n              :key=\"idx\"\n              :title=\"item.title\"\n              :detail=\"item.detail\"\n              :period=\"item.period\"\n            />\n          </div>\n          <div class=\"info-wrap\">\n            <h3 class=\"info-title\">license</h3>\n            <InfoItem\n              v-for=\"(item, idx) in licenseItems\"\n              :key=\"idx\"\n              :title=\"item.title\"\n              :detail=\"item.detail\"\n              :period=\"item.period\"\n            />\n          </div>\n        </div>\n      </section>\n      <!-- about-section END -->\n\n      <!-- passion-section START -->\n      <section class=\"passion-section\">\n        <div class=\"l-center\">\n          <h5 class=\"passion-txt\" data-aos=\"fade-up\" data-aos-duration=\"800\">\n            도전을 두려워하지 않는 <br>\n          </h5>\n          <h5 class=\"passion-txt\" data-aos=\"fade-up\" data-aos-duration=\"800\" data-aos-delay=\"300\">\n            열정❤️‍🔥 가득한 웹 퍼블리셔,<br>\n          </h5>\n          <h5 class=\"passion-txt\" data-aos=\"fade-up\" data-aos-duration=\"800\" data-aos-delay=\"600\">\n            김지희입니다.\n          </h5>\n        </div>\n      </section>\n      <!-- passion-section END -->\n    </div>\n  </div>\n</template>\n<script>\nimport InfoItem from './components/InfoItem.vue'\nimport { gsap } from 'gsap'\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\nimport 'gsap/ScrollToPlugin';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: \"Index\",\n  components: {\n    InfoItem,\n  },\n  data() {\n    return {\n      workItems: [\n        {\n          title: '포켓컴퍼니',\n          detail: '웹 퍼블리셔 / 사원',\n          period: '2023.09 - 2024.10'\n        },\n        {\n          title: '(주)씨케이앤비',\n          detail: '웹 디자이너 / 대리, 파트장',\n          period: '2019.03 - 2021.01'\n        },\n        // {\n        //   title: '(주)대해이앤지',\n        //   detail: '경리 / 대리',\n        //   period: '2015.11 - 2018.09'\n        // },\n        // {\n        //   title: '특허법인 누리',\n        //   detail: '특허 도면사 / 대리',\n        //   period: '2012.01 - 2014.02'\n        // },\n        // {\n        //   title: 'J&K 국제특허 사무소',\n        //   detail: '특허 도면사 / 대리',\n        //   period: '2009.07 - 2011.12'\n        // }\n      ],\n      eduItems: [\n        {\n          title: '리액트&Node.js',\n          period: '2023.04 - 2023.06'\n        },\n        {\n          title: 'JQuery&Javascript',\n          period: '2022.04 - 2022.06'\n        },\n        {\n          title: 'JQuery&Javascript',\n          period: '2020.10 - 2020.12'\n        },\n        {\n          title: 'HTML&CSS',\n          period: '2020.05 - 2020.08'\n        },\n        {\n          title: '웹표준&접근성',\n          period: '2020.02 - 2020.03'\n        },\n      ],\n      licenseItems: [\n        {\n          title: '컴퓨터 그래픽스 운용기능사',\n          period: '2018.09'\n        },\n      ],\n    }\n  },\n  mounted: function() {\n    this.scrollAnimation();\n    this.textScrollAnimation();\n    this.viewColorChange();\n  },\n  methods: {\n    scrollAnimation() {\n      ScrollTrigger.config({autoRefreshEvents: \"visibilitychange,DOMContentLoaded,load\"});\n      const containers = gsap.utils.toArray(\".intro-section\");\n\n      containers.forEach((cont, i) => {\n        const slides = Array.from(cont.querySelectorAll(\".slide\"));\n        const innerHeight = window.innerHeight;\n\n        // 전체 애니메이션 범위 계산\n        const marginValue = innerHeight * (slides.length - 1);\n        const totalScrollHeight = marginValue + innerHeight;\n\n\n        ScrollTrigger.create({\n          trigger: cont,\n          start: \"top top\",\n          end: \"+=\" + totalScrollHeight,\n          pin: true,\n          pinSpacing: true, // 섹션 간의 간격 유지\n          markers: true,\n        });\n\n        // 애니메이션 타임라인 생성\n        gsap\n          .timeline({\n            scrollTrigger: {\n              trigger: cont,\n              start: \"top top\",\n              end: \"+=\" + totalScrollHeight,\n              scrub: true,\n              markers: true,\n            },\n          })\n          .to(slides, {\n            xPercent: -(100 * (slides.length - 1)), // 슬라이드가 연속적으로 이동\n            duration: slides.length,\n            ease: \"none\",\n          });\n      });\n    },\n\n    textScrollAnimation() {\n      gsap.to(\".txt01\", {\n        scrollTrigger: {\n        trigger: \".intro-txt-grp\",\n        start: \"top center\",  // 텍스트가 화면 중간에 들어오면 시작\n        end: \"bottom center\",\n        scrub: true,\n        markers: true,         // 디버깅을 위해 마커 활성화\n        // scroller: \".page\",\n      },\n      x: 130,\n      ease: \"none\"\n      });\n\n      gsap.to(\".txt02\", {\n        scrollTrigger: {\n          trigger: \".intro-txt-grp\",\n          start: \"top center\",  // 텍스트가 화면 중간에 들어오면 시작\n          end: \"bottom center\",\n          scrub: true,\n          markers: true\n        },\n        x: 300, // 이동 거리\n        ease: \"none\"\n      });\n\n      gsap.to(\".txt03\", {\n        scrollTrigger: {\n          trigger: \".intro-txt-grp\",\n          start: \"top center\",  // 텍스트가 화면 중간에 들어오면 시작\n          end: \"bottom center\",\n          scrub: true,\n          markers: true\n        },\n        x: 200, // 이동 거리\n        ease: \"none\"\n      });\n    },\n\n    // 화면 색깔 전환 애니메이션\n    viewColorChange() {\n      gsap.to(\".change\", {\n        scrollTrigger: {\n          trigger: \".change\",\n          start: \"center center\",\n          end: \"bottom bottom\",\n          scrub: true,\n          markers: true\n        },\n        duration: 0.01,\n        backgroundColor: \"#fff\",\n        ease: \"none\",\n      });\n    },\n  }\n};\n</script>"],"mappings":"AAuIA,OAAOA,QAAO,MAAO,2BAA0B;AAC/C,SAASC,IAAG,QAAS,MAAK;AAC1B,SAASC,aAAY,QAAS,oBAAoB;AAClD,OAAO,qBAAqB;AAC5BD,IAAI,CAACE,cAAc,CAACD,aAAa,CAAC;AAClC,eAAe;EACbE,IAAI,EAAE,OAAO;EACbC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,CACT;QACEC,KAAK,EAAE,OAAO;QACdC,MAAM,EAAE,aAAa;QACrBC,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,UAAU;QACjBC,MAAM,EAAE,kBAAkB;QAC1BC,MAAM,EAAE;MACV;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MAAA,CACD;MACDC,QAAQ,EAAE,CACR;QACEH,KAAK,EAAE,aAAa;QACpBE,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,mBAAmB;QAC1BE,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,mBAAmB;QAC1BE,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,UAAU;QACjBE,MAAM,EAAE;MACV,CAAC,EACD;QACEF,KAAK,EAAE,SAAS;QAChBE,MAAM,EAAE;MACV,CAAC,CACF;MACDE,YAAY,EAAE,CACZ;QACEJ,KAAK,EAAE,gBAAgB;QACvBE,MAAM,EAAE;MACV,CAAC;IAEL;EACF,CAAC;EACDG,OAAO,EAAE,SAAAA,CAAA,EAAW;IAClB,IAAI,CAACC,eAAe,CAAC,CAAC;IACtB,IAAI,CAACC,mBAAmB,CAAC,CAAC;IAC1B,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACPH,eAAeA,CAAA,EAAG;MAChBZ,aAAa,CAACgB,MAAM,CAAC;QAACC,iBAAiB,EAAE;MAAwC,CAAC,CAAC;MACnF,MAAMC,UAAS,GAAInB,IAAI,CAACoB,KAAK,CAACC,OAAO,CAAC,gBAAgB,CAAC;MAEvDF,UAAU,CAACG,OAAO,CAAC,CAACC,IAAI,EAAEC,CAAC,KAAK;QAC9B,MAAMC,MAAK,GAAIC,KAAK,CAACC,IAAI,CAACJ,IAAI,CAACK,gBAAgB,CAAC,QAAQ,CAAC,CAAC;QAC1D,MAAMC,WAAU,GAAIC,MAAM,CAACD,WAAW;;QAEtC;QACA,MAAME,WAAU,GAAIF,WAAU,IAAKJ,MAAM,CAACO,MAAK,GAAI,CAAC,CAAC;QACrD,MAAMC,iBAAgB,GAAIF,WAAU,GAAIF,WAAW;QAGnD5B,aAAa,CAACiC,MAAM,CAAC;UACnBC,OAAO,EAAEZ,IAAI;UACba,KAAK,EAAE,SAAS;UAChBC,GAAG,EAAE,IAAG,GAAIJ,iBAAiB;UAC7BK,GAAG,EAAE,IAAI;UACTC,UAAU,EAAE,IAAI;UAAE;UAClBC,OAAO,EAAE;QACX,CAAC,CAAC;;QAEF;QACAxC,IAAG,CACAyC,QAAQ,CAAC;UACRC,aAAa,EAAE;YACbP,OAAO,EAAEZ,IAAI;YACba,KAAK,EAAE,SAAS;YAChBC,GAAG,EAAE,IAAG,GAAIJ,iBAAiB;YAC7BU,KAAK,EAAE,IAAI;YAC