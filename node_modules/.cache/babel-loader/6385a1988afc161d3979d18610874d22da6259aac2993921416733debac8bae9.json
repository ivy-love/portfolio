     </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"LayoutHeader\",\r\n  data() {\r\n    return {\r\n      isOpen: false,\r\n      isActive: false,\r\n      gnbList: ['About', 'Works', 'Contact'],\r\n    }\r\n  },\r\n  mounted () {\r\n    this.worksAni();\r\n    this.txtScroll();\r\n    this.goMenu();\r\n    this.handleScroll();\r\n  },\r\n  methods: {\r\n    // 모바일 메뉴 열렸을 때 body overflow 추가 및 제거\r\n    toggleMoBtn() {\r\n      this.isOpen = !this.isOpen;\r\n      if (this.isOpen) {\r\n        document.body.classList.add('is-active');\r\n      }\r\n      else {\r\n        document.body.classList.remove('is-active')\r\n      }\r\n    },\r\n\r\n    // works-section 애니메이션\r\n    worksAni() {\r\n      let worksSection = document.querySelector('.works-section');\r\n      window.addEventListener('scroll', function() {\r\n        let sectionRect = worksSection.getBoundingClientRect();\r\n        if (sectionRect.top * 0.5 <= window.innerHeight && sectionRect.bottom >= 0) {\r\n          worksSection.classList.add('is-active');\r\n        } else {\r\n          worksSection.classList.remove('is-active');\r\n        }\r\n      });\r\n    },\r\n\r\n    // gnb메뉴 text-section 지났을 때 클래스 추가\r\n    txtScroll() {\r\n      let header = document.querySelector('.portfolio-header');\r\n      let textSection = document.querySelector('.text-section');\r\n      const vm = this;\r\n\r\n      window.addEventListener('scroll', function() {\r\n        if (vm.isOpen) {\r\n          header.classList.remove('is-color');\r\n          return;\r\n        }\r\n\r\n        let sectionRect = textSection.getBoundingClientRect();\r\n\r\n        if (sectionRect.top <= 0) {\r\n          header.classList.add('is-color');\r\n        } else {\r\n          header.classList.remove('is-color');\r\n        }\r\n      });\r\n    },\r\n\r\n    // 메뉴 이동 버튼\r\n    goMenu(section) {\r\n      const targetSection = document.querySelector(`#${section}`);\r\n      if (targetSection) {\r\n        targetSection.scrollIntoView({ behavior: \"smooth\" });\r\n      }\r\n    },\r\n\r\n    // 스크롤 시 헤더 숨기기\r\n    handleScroll () {\r\n      let lastScrollY = 0;\r\n      const vm = this;\r\n\r\n      window.addEventListener(\"scroll\", function () {\r\n        if (vm.isOpen) return;\r\n\r\n        let header = document.querySelector(\".portfolio-header\");\r\n        let currentScrollY = window.scrollY;\r\n\r\n        if (currentScrollY === 0 || currentScrollY < lastScrollY || currentScrollY < 84) {\r\n            header.classList.remove(\"is-scroll\");\r\n        } else if (currentScrollY > lastScrollY) {\r\n            header.classList.add(\"is-scroll\");\r\n        }\r\n\r\n        lastScrollY = currentScrollY;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n"],"mappings":";;;EAESA,KAAK,EAAC;AAAW;;EACfA,KAAK,EAAC;AAAS;;EACdA,KAAK,EAAC;AAAM;;EACXA,KAAK,EAAC;AAAkB;;EACvBA,KAAK,EAAC;AAAU;;uBAL5BC,mBAAA,CAmBM;IAnBDD,KAAK,EADZE,eAAA,EACa,kBAAkB;MAAA,YAAuBC,KAAA,CAAAC;IAAQ;MAC1DC,mBAAA,CAiBM,OAjBNC,UAiBM,GAhBJD,mBAAA,CAeM,OAfNE,UAeM,GAdJF,mBAAA,CAAmF,MAAnFG,UAAmF,GAAlEH,mBAAA,CAA6D;IAAzDI,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,IAAA,CAAAC,OAAO,CAACC,IAAI;MAAAC,IAAA;IAAA;gCAJhDC,gBAAA,CAIqE,MAAI,GAAAX,mBAAA,CAAI,qCAJ7EW,gBAAA,CAI6E,OAAK,E,MAC1EX,mBAAA,CAOM,OAPNY,UAOM,GANJZ,mBAAA,CAKK,MALLa,UAKK,I,kBAJHjB,mBAAA,CAGKkB,SAAA,QAVjBC,WAAA,CAOgDjB,KAAA,CAAAkB,OAAO,EAAfC,IAAI;yBAAhCrB,mBAAA,CAGK;MAHDD,KAAK,EAAC,UAAU;MAA0BuB,GAAG,EAAED;QACjDE,mBAAA,8FAA2F,EAC3FnB,mBAAA,CAAmF;MAAhFoB,IAAI,EAAC,oBAAoB;MAACzB,KAAK,EAAC,UAAU;MAAE0B,aAAY,EAAAhB,MAAA,QAAAA,MAAA,UAAAiB,IAAA,KAAEC,QAAA,CAAAC,MAAA,IAAAD,QAAA,CAAAC,MAAA,IAAAF,IAAA,CAAM;wBAAKL,IAAI,kC;sCAIlFjB,mBAAA,CAIS;IAJDyB,IAAI,EAAC,QAAQ;IAAC9B,KAAK,EAbnCE,eAAA,EAaoC,qBAAqB;MAAA,aAAwBC,KAAA,CAAA4B;IAAM;IAAKtB,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAiB,IAAA,KAAEC,QAAA,CAAAI,WAAA,IAAAJ,QAAA,CAAAI,WAAA,IAAAL,IAAA,CAAW;gCACpGtB,mBAAA,CAEM;IAFDL,KAAK,EAAC;EAAa,IACtBK,mBAAA,CAA6B;IAAvBL,KAAK,EAAC;EAAS,G","ignoreList":[]